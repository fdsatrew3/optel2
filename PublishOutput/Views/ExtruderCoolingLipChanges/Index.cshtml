@model IEnumerable<Optel2.Models.ExtruderCoolingLipChange>

@{
    ViewBag.Title = "Extruders cooling lip data";
}

<h2>Extruders cooling lip data</h2>

<p>
    @Html.ActionLink("Create new extruder cooling lip data", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Cooling Lip
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Duration)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Consumption)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Extruder)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CoolingLip)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Duration)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Consumption)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Extruder.Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
